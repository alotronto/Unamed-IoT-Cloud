{
   "controller": {
   },
   "workers": 
   [
      {
         "type": "router",
         "realms": 
         [
            {
               "name": "realmName",
               "roles": 
               [
                  {
                     "name": "anonymous",
                     "permissions": 
                     [
                        {
                           "uri": "*",
                           "publish": true,
                           "subscribe": true,
                           "call": true,
                           "register": true
                        }
                     ]
                  }
               ]
            }
		   ],
         "transports": 
         [
            {
               "type": "websocket",
               "endpoint": 
               {
                  "type": "tcp",
                  "port": 8181
               },
               "debug":true,
               "options":
               {
                  "enable_webstatus":true,
                  "fail_by_drop": true,
			         "open_handshake_timeout": 2500,
      			   "close_handshake_timeout": 1000,
                  "auto_ping_interval": 10000,
                  "auto_ping_timeout": 5000,
                  "auto_ping_size": 4
               }
            }
         ]
      }
   ]
}
